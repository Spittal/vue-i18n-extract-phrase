{
  "name": "vue-i18n-phrase",
  "version": "0.2.0",
  "description": "Extract all vue-i18n keys from your Vue.js projects. Then, add missing keys to your Phrase account!",
  "scripts": {
    "build": "pack build",
    "demo": "node ./pkg/dist-node/index.bin.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint src/**/*",
    "dist": "pack publish --yolo"
  },
  "homepage": "https://github.com/Spittal/vue-i18n-phrase",
  "repository": "https://github.com/Spittal/vue-i18n-phrase",
  "author": "Jamie Spittal <jamie@jamiespittal.com>",
  "bugs": {
    "url": "https://github.com/Spittal/vue-i18n-phrase/issues"
  },
  "keywords": [
    "vue i18n",
    "vue",
    "vue.js",
    "plugin",
    "internationalization",
    "internationalization plugin",
    "phrase",
    "phrase app"
  ],
  "license": "MIT",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node",
        {
          "minNodeVersion": "10"
        }
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "vue-i18n-phrase"
        }
      ]
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "form-data": "^2.3.3",
    "vue-i18n-extract": "0.4.9"
  },
  "devDependencies": {
    "@pika/pack": "^0.3.4",
    "@pika/plugin-build-node": "^0.3.4",
    "@pika/plugin-simple-bin": "^0.3.13",
    "@pika/plugin-ts-standard-pkg": "^0.3.4",
    "@types/form-data": "^2.2.1",
    "jest": "^24.3.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "typescript": "^3.2.2"
  }
}
