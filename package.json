{
  "name": "vue-i18n-phrase",
  "version": "0.3.7",
  "description": "Extract all vue-i18n keys from your Vue.js projects. Then, add missing keys to your Phrase account!",
  "scripts": {
    "build": "pika build",
    "demo": "node ./pkg/dist-node/index.bin.js",
    "lint": "tslint src/**/*",
    "dist": "pika publish --yolo"
  },
  "homepage": "https://github.com/Spittal/vue-i18n-phrase",
  "repository": "https://github.com/Spittal/vue-i18n-phrase",
  "author": "Jamie Spittal <jamie@jamiespittal.com>",
  "bugs": {
    "url": "https://github.com/Spittal/vue-i18n-phrase/issues"
  },
  "keywords": [
    "vue i18n",
    "vue",
    "vue.js",
    "plugin",
    "internationalization",
    "internationalization plugin",
    "phrase",
    "phrase app"
  ],
  "license": "MIT",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node",
        {
          "minNodeVersion": "10"
        }
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "vue-i18n-phrase"
        }
      ]
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "commander": "^3.0.0",
    "form-data": "^2.5.0",
    "vue-i18n-extract": "1.0.2"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.0",
    "@pika/plugin-simple-bin": "^0.6.0",
    "@pika/plugin-ts-standard-pkg": "^0.6.0",
    "@types/form-data": "^2.5.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
